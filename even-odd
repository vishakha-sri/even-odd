import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner in = new Scanner(System.in);
        System.out.print("Enter any number");
        int num = in.nextInt();
        if(num%2 == 0){
            System.out.println(num+" is a even no.");
        }else{
            System.out.println(num+" is an odd no.");
        }

    }
}
